msgid "Original: %s"
msgstr "Ursprünglicher Name: %s"

#: wp-admin/plugin-editor.php:156 wp-admin/theme-editor.php:114
msgid "Function Name&hellip;"
msgstr "Funktionsname&#160;&hellip;"

